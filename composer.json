{
  "name": "larva/hyperf-settings",
  "description": "This is hyperf settings module.",
  "keywords": [
    "hyperf",
    "settings",
    "larva"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">= 7.2",
    "hyperf/config": "^1.0|^2.0",
    "hyperf/database": "^1.0|^2.0",
    "hyperf/db-connection": "^1.0|^2.0",
    "hyperf/utils": "^1.0|^2.0"
  },
  "require-dev": {
    "swoole/ide-helper": "^4.5"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Settings\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "hyperf": {
      "config": "Larva\\Settings\\ConfigProvider"
    }
  },
  "prefer-stable": true
}
